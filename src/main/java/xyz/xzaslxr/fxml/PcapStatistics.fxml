<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="338.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xyz.xzaslxr.fxml.controller.PcapStatisticsController">
   <children>
      <PieChart fx:id="pcapPieChart" layoutX="422.0" layoutY="14.0" prefHeight="250.0" prefWidth="350.0" title="数据分布图（按协议划分）" />
      <BarChart fx:id="pcapBarChart" layoutX="59.0" layoutY="25.0" prefWidth="350.0" title="数据统计图（数据包/时间）">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" label="时间" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" label="数据包数量"  />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
